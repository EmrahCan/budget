# Production Environment for Azure VM
NODE_ENV=production

# Frontend Configuration
REACT_APP_API_URL=http://[VM-PUBLIC-IP]:5001/api
REACT_APP_ENVIRONMENT=production
REACT_APP_VERSION=2.0.0

# Backend Configuration
PORT=5001
FRONTEND_URL=http://[VM-PUBLIC-IP]:3002

# Database Configuration (Azure MySQL)
DB_HOST=budget-mysql-server.mysql.database.azure.com
DB_PORT=3306
DB_NAME=budget_app
DB_USER=budgetadmin
DB_PASSWORD=YOUR_MYSQL_PASSWORD_HERE

# Security
JWT_SECRET=BudgetApp2024SecureJWTKey!@#$%

# AI Configuration
GEMINI_API_KEY=YOUR_GEMINI_API_KEY_HERE
AI_USE_MOCK_DATA=false

# CORS Configuration
CORS_ORIGIN=http://[VM-PUBLIC-IP]:3002

# Logging
LOG_LEVEL=info